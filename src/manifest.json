{
	"manifest_version": 2,
	"name": "Focus",
	"version": "1.0",
	"author": "Michael Zhao",

	"description": "A focused new tab experience that allows for you to be the most productive that you can :D",

	"icons": {
		"48": "icon.png"
	},

	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},

	"content_security_policy": "script-src 'self' 'sha256-BKb1CxC6Orzrdo9H+kWQgXMBexNrH4RQemboEiJo6Q4=' https://fonts.google.com; object-src 'self'",

	"permissions": ["<all_urls>"]
}
