{
	"manifest_version": 2,
	"name": "Focus",
	"version": "1.0.2",
	"author": "Michael Zhao",

	"description": "A focused new tab experience that allows for you to be the most productive that you can :D",

	"icons": {
		"48": "icon.png"
	},

	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},

	"content_security_policy": "script-src 'self' 'sha256-BKb1CxC6Orzrdo9H+kWQgXMBexNrH4RQemboEiJo6Q4=' https://fonts.google.com https://cdnjs.cloudflare.com; object-src 'self'",

	"options_ui": {
		"page": "options.html",
		"browser_style": true,
		"chrome_style": true
	},

	"permissions": ["<all_urls>", "storage"],

	"browser_specific_settings": {
		"gecko": {
			"id": "focus@michaelzhao.xyz",
			"strict_min_version": "70.0"
		}
	}
}
